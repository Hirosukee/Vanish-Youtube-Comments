{

  "manifest_version": 2,
  "name": "Vanish Youtube Comments",
  "version": "1.0",
 
  "description": "Vanish youtube comments from your screen.",
  
  "icons": {
    "48": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["script.js"]
    }
  ]

}